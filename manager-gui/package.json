{
    "name": "vagrant-repository-manager-gui",
    "version": "0.0.1",
    "description": "Vagrant Repository Manager GUI",
    "author": "Thomas Johansen <thomas.johansen@accenture.com>",
    "license": "Apache-2.0",
    "private": true,
    "main": "index.js",
    "keywords": [
        "react",
        "redux",
        "typescript",
        "semantic ui",
        "webpack"
    ],
    "scripts": {
        "clean": "rm -rf ./dist && mkdir ./dist",
        "build": "webpack -p --progress --colors",
        "start": "webpack-dev-server --mode production --hot --progress --color --host 0.0.0.0 --port 3000",
        "dev-client": "webpack-dev-server --mode development --hot --progress --color --host 0.0.0.0 --port 3000",
        "dev-server": "nodemon ./src/server/index.js 8080",
        "dev-start": "concurrently \"yarn dev-server\" \"yarn dev-client\""
    },
    "dependencies": {
        "axios": "^0.18.0",
        "lodash": "^4.17.11",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-redux": "^5.0.7",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^4.0.8",
        "redux": "^3.7.2",
        "redux-actions": "^2.6.1",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^3.0.1",
        "semantic-ui-css": "^2.3.3",
        "semantic-ui-react": "^0.82.3"
    },
    "devDependencies": {
        "@types/node": "^10.9.4",
        "@types/react": "^16.4.13",
        "@types/react-dom": "^16.0.7",
        "@types/react-redux": "^6.0.6",
        "@types/react-router": "^4.0.30",
        "@types/react-router-dom": "^4.3.0",
        "@types/react-router-redux": "^5.0.15",
        "@types/redux-actions": "^2.3.0",
        "autoprefixer": "^9.1.5",
        "body-parser": "^1.18.3",
        "concurrently": "^4.0.1",
        "css-loader": "^1.0.0",
        "express": "^4.16.4",
        "file-loader": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^1.18.4",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.0",
        "ts-loader": "^5.0.0",
        "tslint": "^5.11.0",
        "typescript": "^3.0.3",
        "url-loader": "^1.1.1",
        "webpack": "^4.17.1",
        "webpack-cleanup-plugin": "^0.5.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.7"
    }
}
